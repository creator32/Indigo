@model TravelerPortal.Data.Album
@{
    ViewBag.Title = "Photogallery";
}

<h1 style="margin-left: 0px;">@Model.Name</h1>
@foreach (var image in Model.AlbumImages)
{
    <div class="thumbnail">
        <img class="no-border" src="@image.Thumbnail" width="60" height="60">
        <br>
        <a href="">@image.Name</a>
    </div>
}